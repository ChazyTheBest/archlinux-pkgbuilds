  # commons-proxy
  install -Dm644 ${srcdir}/commons-proxy-1.0/commons-proxy-1.0.jar ${pkgdir}/usr/share/java/commons/proxy.jar
  install -Dm644 ${srcdir}/commons-proxy-1.0/commons-proxy-1.0-javadoc.jar ${pkgdir}/usr/share/java/commons/javadoc/proxy-javadoc.jar
  install -Dm644 ${srcdir}/commons-proxy-1.0/commons-proxy-1.0-sources.jar ${pkgdir}/usr/share/java/commons/sources/proxy-sources.jar

  # commons-scxml
  install -Dm644 ${srcdir}/commons-scxml-0.9/commons-scxml-0.9.jar ${pkgdir}/usr/share/java/commons/scxml.jar
  install -Dm644 ${srcdir}/commons-scxml-0.9/commons-scxml-0.9-javadoc.jar ${pkgdir}/usr/share/java/commons/javadoc/scxml-javadoc.jar
  install -Dm644 ${srcdir}/commons-scxml-0.9/commons-scxml-0.9-sources.jar ${pkgdir}/usr/share/java/commons/sources/scxml-sources.jar

  # commons-transaction
  install -Dm644 ${srcdir}/commons-transaction-1.2/commons-transaction-1.2.jar ${pkgdir}/usr/share/java/commons/transaction.jar

  # commons-validator
  install -Dm644 ${srcdir}/commons-validator-1.4.0/commons-validator-1.4.0.jar ${pkgdir}/usr/share/java/commons/validator.jar
  install -Dm644 ${srcdir}/commons-validator-1.4.0/commons-validator-1.4.0-javadoc.jar ${pkgdir}/usr/share/java/commons/javadoc/validator-javadoc.jar
  install -Dm644 ${srcdir}/commons-validator-1.4.0/commons-validator-1.4.0-sources.jar ${pkgdir}/usr/share/java/commons/sources/validator-sources.jar

  # commons-vfs
  install -Dm644 ${srcdir}/commons-vfs-2.0/commons-vfs2-2.0.jar ${pkgdir}/usr/share/java/commons/vfs.jar
  install -Dm644 ${srcdir}/commons-vfs-2.0/commons-vfs2-2.0-javadoc.jar ${pkgdir}/usr/share/java/commons/javadoc/vfs-javadoc.jar
  install -Dm644 ${srcdir}/commons-vfs-2.0/commons-vfs2-2.0-sources.jar ${pkgdir}/usr/share/java/commons/sources/vfs-sources.jar
  install -Dm644 ${srcdir}/commons-vfs-2.0/commons-vfs2-2.0-tests.jar ${pkgdir}/usr/share/java/commons/tests/vfs-tests.jar
  install -Dm644 ${srcdir}/commons-vfs-2.0/commons-vfs2-2.0-test-sources.jar ${pkgdir}/usr/share/java/commons/sources/vfs-test-sources.jar
  install -Dm644 ${srcdir}/commons-vfs-2.0/commons-vfs2-examples-2.0.jar ${pkgdir}/usr/share/java/commons/examples/vfs-examples.jar
  install -Dm644 ${srcdir}/commons-vfs-2.0/commons-vfs2-examples-2.0-javadoc.jar ${pkgdir}/usr/share/java/commons/javadoc/vfs-examples-javadoc.jar
  install -Dm644 ${srcdir}/commons-vfs-2.0/commons-vfs2-examples-2.0-sources.jar ${pkgdir}/usr/share/java/commons/sources/vfs-examples-sources.jar

}