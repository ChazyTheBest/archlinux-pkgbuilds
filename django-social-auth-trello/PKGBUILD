pkgname=python2-django-social-auth-trello
pkgver=1.0.2
pkgrel=1
pkgdesc="A Sentry plugin that integrats with Trello."
arch=(any)
url="http://pypi.python.org/pypi/django-social-auth-trello"
license=(GPL)
depends=(python2)
makedepends=(python2-distribute)
source=("http://pypi.python.org/packages/source/d/django-social-auth-trello/django-social-auth-trello-${pkgver}.tar.gz")
md5sums=(db6a67aea370d21face1c72fd41293b0)

build () {
    
    cd "${srcdir}/django-social-auth-trello-${pkgver}"
    python2 setup.py install --root="${pkgdir}/" --optimize=1
}